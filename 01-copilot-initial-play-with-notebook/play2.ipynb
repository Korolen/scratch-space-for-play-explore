    "# import library px\n",
    "import plotly.express as px\n",
    "# dataset\n",
    "df = px.data.tips()\n",
    "# plot\n",
    "fig = px.scatter(df, x=\"total_bill\", y=\"tip\", color=\"smoker\", marginal_y=\"violin\", marginal_x=\"box\", trendline=\"ols\")\n",
    "# show plot\n",
    "fig.show()"
